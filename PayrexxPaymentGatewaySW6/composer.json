{
  "name": "payrexx/payment",
  "description": "A Shopware plugin to accept payments with Payrexx",
  "version": "1.0.3",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Payrexx AG",
      "homepage": "https://payrexx.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "shopware/core": "^6.1",
    "shopware/administration":"^6.1",
    "payrexx/payrexx": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "PayrexxPaymentGateway\\": "src/"
    }
  },
  "extra": {
    "shopware-plugin-class": "PayrexxPaymentGateway\\PayrexxPaymentGatewaySW6",
    "label": {
      "en-GB": "Payrexx Payment Gateway",
      "de-DE": "Payrexx Payment Gateway"
    }
  }
}
